{
    "name": "subiabre/apio",
    "description": "Minimalist component based library to write web APIs.",
    "type": "library",
    "keywords": [
        "library",
        "web",
        "API",
        "simple"
    ],
    "support": {
        "email": "subiabrewd@gmail.com"
    },
    "require": {
        "nikic/fast-route": "^1.3",
        "symfony/http-foundation": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "symfony/console": "^5.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Facu",
            "email": "subiabrewd@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Apio\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Apio\\Tests\\": "tests/" }
    },
    "bin": ["apio"],
    "config": {
        "bin-dir": "./"
    }
}
